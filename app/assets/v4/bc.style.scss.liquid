    }
    .animated-10 {      
      @include transition(transform 1s cubic-bezier(0.165, 0.84, 0.44, 1) 0.76s, opacity 1s cubic-bezier(0.165, 0.84, 0.44, 1) 0.86s);
    }
    .animated-11 {      
      @include transition(transform 1s cubic-bezier(0.165, 0.84, 0.44, 1) 0.82s, opacity 1s cubic-bezier(0.165, 0.84, 0.44, 1) 0.92s);
    }
    .animated-12 {      
