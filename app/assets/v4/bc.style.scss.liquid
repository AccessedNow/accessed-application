.templateIndex {     
  #body-content {   
    /*@include translate3d(0, 0, 0);   */   
  }
  .header-navigation .table-row .vertical-menu .navigation .head {
    @include border-radius(10px, 10px, 0, 0);
    &.opened {
      @include border-radius-10;
    }
  }
  .style-1, .style-2 {
    .header-navigation .table-row .vertical-menu .sidemenu-holder .navigation {
      .navbar {
        display: block;       
        @include dropdown-effect-hover;
        @include translate(0, 0);
        &.opened {
          display: none;
          @include dropdown-effect;
          @include translate(0, 10px);
        }
      }     
    }  
  }   
  .header-content .header-container.layout-boxed.style-2 .header-navigation .table-row .vertical-menu .navigation .head {
    @include border-radius(10px, 0, 0, 0);
    &.opened {
      @include border-radius(10px, 0, 0, 10px);
    }
  }  
  .header-content .style-7 {
    .header-main .vertical-menu .sidemenu-holder .navigation {
      .navbar {         
        @include dropdown-effect-hover;
        @include translate(0, 0);
        &.opened {          
          @include dropdown-effect;
          @include translate(0, 10px);
        }
      } 
    } 
    &.header-fixed {
      .header-main .vertical-menu .sidemenu-holder .navigation {
        .navbar {  
          @include dropdown-effect;
          @include translate(0, 10px);       		        
          &.opened {          
            @include dropdown-effect-hover;
            @include translate(0, 0);
          }
        } 
      }
    }
  } 
  #footer-content {
    margin-top: 0;
    padding-top: 38px;
    @media (max-width:767px) {
      padding-top: 15px;
    }
  }
}
