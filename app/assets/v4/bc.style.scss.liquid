        color: $body-title-color;
        @include border-radius-reset;    
      }
    }  
    a {      
      line-height: 24px;
      margin: 0;
      padding: 0 0 9px;                      
      border: none;               
      display: block;
      position: relative;          
      color: $body-title-color;
      font-size: $size-h5;                                                         
      background: none;    
      text-align: center;   
      @include border-radius-reset; 
      &:before {
        content: '';
        width: 0;
        height: 2px;
        z-index: 1;
        background-color: $main-color;
        @include ab(n, 50%, 0, 50%);
        @include transition(all 0.3s ease 0s);
        @include opacity(0);    
      }  
      &:after {
        content: "\e820";            
        color: $main-color;               
        margin: 0 auto;
        height: 10px;
        line-height: 10px;
        font-family: arenafont;
        @include ab(n, 0, -7px, 0);
        @include transition(all 0.3s ease 0s);
        @include opacity(0);
      }     
      &:hover {
        border: none;
        background: none;        
      }
      &:focus {
        border: none;
        background: none;          
        color: $body-title-color; 
      }
      @media (min-width: 1025px) {
        &:hover {            
          color: $body-title-color;
          &:before {
            @include opacity(1);
            left: 0;
            right: 0;
            width: 100%;
          }
          &:after {
            @include opacity(1);
            z-index: 1;
          }     
        }
      }      
      &.active {
        background: none;
        border: none;
        color: $body-title-color;
        font-weight: $font-bold;          
        &:before {
          @include opacity(1);
          left: 0;
          right: 0;
          width: 100%;
        }
        &:after {
          @include opacity(1);
          z-index: 1;
        }    
      }
    } 
  }    
  &.tab-title-1,
  &.tab-title-3 {
    li {
      padding: 0 20px;
      @media (max-width: 991px) {
        padding: 0 10px;
      }
      &:first-child {
        padding-left: 0;
      }
      &:last-child {
        padding-right: 0;
      }
    }   
  }  
  &.tab-title-2,
  &.tab-title-4 {
    padding: 0 0 15px;
    border-bottom: none;
    @media (max-width: 1024px) {
      &:before {
        content: none !important;
      }
    }
    li {
      padding: 0;
      .nav-link {
        line-height: 24px;
        margin: 0;
        padding: 3px 20px;      
        font-weight: 400;
        font-size: $body-font-size + 1px;
        position: relative;
        border: 2px solid transparent;
        @include border-radius(13px, 13px, 13px, 13px);
        @media (max-width: 991px) {
          padding-left: 12px;
          padding-right: 12px;
        }
        &:before,
        &:after {
          content: none;
        }
        &.active,
        &.show {
          border-color: $main-color;
        }
        &:hover {
          @media (min-width: 1025px) {
            border-color: $main-color;
          }
        }
      }
    }           
  }
  &.tab-title-4 {
    li {     
      .nav-link {
        &.active,
        &.show {
          background: $main-color;
          color: $main-color-text;
          font-weight: $font-bold;
        }        
      }
    }
  }
