    }
    &.image-3 {
      text-align: left;
      left: -5% !important;
      top: auto !important;
      bottom: 0 !important;
      right: auto !important;
      max-width: 50%;
      margin: 0 auto !important;
      @media (max-width: 1200px) {
        left: -2% !important;
      }
    }
    &.tp-parallax-text {
      top: 35% !important;
      z-index: 2;
      padding: 0 15px;
      @include preserve-3d;  
      .heading-text {
        margin-bottom: 1%;
      }
      .custom-block-heading-text {
        margin-bottom: 0;
