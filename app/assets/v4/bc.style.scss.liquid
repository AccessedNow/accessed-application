/* Quick view ----------------------------------*/
.quantity {
  position: relative;
  display: inline-block;
  height: 34px;                     
  border: 1px solid $body-border-color;            
  padding: 0 65px 0 15px;
  @include border-radius(15px, 15px, 15px, 15px);  
  input { 
    width: 100%;   
    padding: 5px 0;
    height: 32px;
    line-height: 18px;
    border: none; 
    text-align: left;
    color: $body-title-color;                     
    background-repeat: no-repeat;
    background-attachment: fixed;
    background: transparent;
    -moz-appearance:textfield;  
    -webkit-appearance:textfield;     
    -webkit-appearance: none; 
    @include border-radius-reset;
    @include placeholder {
      color: $body-title-color; 
    }
    &:focus, 
    &:active,
    &:hover {
      background: none;
    }
  } 
  .qty-wrapper {
    @include ab(n, n, 7px, 5px);        
  }
  .qty-inner {      
    > span {   
      margin: 0 5px 0 0;   
      width: 20px;
      height: 20px;
      line-height: 20px;
      text-align: center;
      padding: 0;            
      cursor: pointer;
      display: inline-block;  
      background: $body-second-color;    
      z-index: 2;      
      @include border-radius-circle;
      i {
        width: 100%;            
        display: block;
        line-height: 20px;  
        font-style: normal;
        font-size: 8px;      
        @include transition(all 0.35s ease 0s);
        &:before {
          font-family: "arenafont";
        }
      }
      &.qty-up {        
        i {
          &:before {
            content: '\e94c';
          }
        }        
      }
      &.qty-down {        
        i {
          &:before {
            content: '\e8d0';
          }
        }
      }
      &:hover {        
        i {
          color: $main-color;
        }
      }
    }
  }
  &.ajax-true {
    padding: 0 10px!important;
    input[type="number"] {            
      padding: 7px 0 !important;
      width: 100%;
    }   
  }
