      overflow-y: auto;      
    }
    > a {
      @include border-radius-circle;    
      @include box-shadow(0 0 0 rgba($black, 0.5));
      @include ab(-25px, n, n, -20px); 
      @include transition(all 0.35s ease 0s);                    
      @include width-btn-1; 
      z-index: 999991;    
      @media (max-width: 767px) {
        top: 0;
        right: 0;
      }
      i {
        &:before {
          @include close-btn;   
          color: $white;                             
        } 
      }
      &:hover {
        background: $button-hover-btn1;
        &:before {
          i {
            &:before {
              color: $button-hover-btn1-txt;
            }
          }
        }
      }
    }
  }
  .modal-body {
    padding: 0;
    display: table;
    width: 100%;
    vertical-align: middle;
    min-height: 300px;    
    table-layout: fixed;
    @media (max-width: 767px) {
      display: block;
      max-height: 100%;
      overflow-y: auto;
    }
    > div {
      display: table-cell;
      vertical-align: middle;
      width: 50%;      
      @media (max-width: 767px) {
        width: 100%;
        display: block;
      }
    }
    .image {
      padding-right: 15px;
      @media (max-width: 767px) {
        padding: 0 0 20px;
        text-align: center;
      }
      img {
        @media (min-width: 768px) {
          @include border-radius(20px, 0, 0, 20px);
        }
        @media (max-width: 767px) {
          @include border-radius(20px, 20px, 0, 0);
        }
      }
    }
    .content {
      padding: 0 15px;
      text-align: center;
      max-width: 300px;
      margin: 0 auto;
      @media (max-width: 767px) {
        padding-bottom: 25px;
      }
      h5 {
        text-transform: uppercase;
        font-weight: 700;
        margin: 0 auto;
      }
      p {
        line-height: 18px;
        margin: 5px 0 13px;
      }
      .btn {
        text-transform: uppercase;
        font-size: $body-font-size - 1px;
        padding: 7px 25px;
        line-height: 28px;        
      }
    }
  }
}
#pre-order-popup {  
  margin: 0 auto;
  max-height: 80vh;
  max-width: 680px;
  overflow: visible;
  background: none;  
  @media (max-width: 767px) {
    overflow: hidden;
    overflow-y: auto;
  }  
  #pre-order-form {
    padding: 40px;
    background: $body-background-color;
    overflow-x: auto;
    overflow-y: auto;
    @include border-radius-25;
    @media (max-width: 479px) {
      padding: 20px;
    }
    .form-control {
      height: 47px;  
      background-repeat: no-repeat;
      background-attachment: fixed;
      font-size: $body-font-size;
      background: $white;
      padding: 13px 30px;   
      line-height: 1.6;   
      color: $body-light-color;
      border: 1px solid $body-border-color;
      -moz-appearance:textfield;  
      -webkit-appearance:textfield;     
      -webkit-appearance: none;   
      @include border-radius-25; 
      @include  box-shadow(0 0 0 0 white inset);
      &:focus {
        border-color: $main-color;
        @include  box-shadow(0 0 0 1000px white inset);
      }
      @include placeholder {
        color: $body-light-color;
      }
    }
    .form-control:-webkit-autofill {
      @include  box-shadow(0 0 0 1000px white inset);
    }
    textarea {
      min-height: 80px;
    }
  }  
}
*:not(img) {  
  &.waiting {
    height: 0;
    position: relative;
    padding-bottom: 70%;
    background-color: transparent;
    &.disable-load,
    &.lazyloaded {
      height: auto;
      padding: 0;
      background-color: transparent;
    }
    img {
      position: absolute;
      left: 0; top: 0;
      width: 100%; height: 100%;
      opacity: 0;
      transition: all 0.3s;       
      &.lazyloaded,
      &.disable-load {
        position:relative;
        height: auto;
        opacity: 1;
      }
    }
  }
}
.lazyloaded {
  img {
    &.lazyloaded {
      width: auto;
    }
  }
}
iframe#preview-bar-iframe {
  display: none;
}
/*Preview Panel*/
.preview-panel {
  position: fixed;
  top: 70px;
  right: 0;  
  z-index: 97;
  width: 35px;    
  @include border-radius(5px, 0, 0, 5px);
  @include transition(all 0.45s cubic-bezier(0.25, 0.46, 0.45, 0.94));   
  #pp-wrapper {
    width: 205px;
    @include translate(205px, 0);   
  }
  .pp-toggle {    
    @include ab(42px, n, n, 0);
    @include border-radius(3px, 0, 0, 3px);
    width: 40px;
    height: 40px;
    background: $body-background-color;   
    color: $body-title-color;
    border: 1px solid $body-border-color;
    border-right: none;
    cursor: pointer;
    text-align: center;
    line-height: 38px;
    i {
      color: inherit;
      font-size: 18px;
      display: block;
      text-align: center;
      line-height: 38px;
    }
  }
  .style-main-title {
    background: #f0f0f0;
    border-bottom: 1px solid #ddd;
    padding: 10px;
    line-height: 22px;
    font-size: $body-font-size + 1px;
    text-align: center;
    color: $black;
    @include border-radius(5px, 0, 0, 0);
  }
  .pp-box {    
    padding: 0 0 15px;
    &:not(:last-child) {
      border-bottom: 1px solid #ddd;
    }
    .box-heading {
      text-align: center;
      font-size: $body-font-size - 1px;
      line-height: 20px;
      padding: 10px;
    }
    .box-menu {
      position: relative;      
      padding: 0 15px 0 40px;
      > label {
        padding: 0 15px 0 5px;
        margin: 0;
        line-height: 22px;
        border: 1px solid #ddd;
        background: #f0f0f0;
        width: 130px;
        cursor: pointer;
        position: relative;
        font-size: $body-font-size - 2px;
        .demo-icon {
          @include ab(0, n, n, 0);
          line-height: 20px;
          width: 15px;
          text-align: center;
        }
      }
      ul {
        margin-top: -1px;
        list-style: none;        
        min-width: 130px;
        border: 1px solid #ddd;
        background: #f0f0f0;
        padding: 6px 0;
       	visibility: hidden; 
        z-index: 2;  
        @include opacity(0);
        @include ab(100%, 40px, n, n);       
        @include transition(all 0.35s ease 0s);
        @include translate(0, 10px);
        li {
          a {
            font-size: $body-font-size - 2px;
            line-height: 16px;
            padding: 2px 5px;
            display: block;
          }
        }
      }      
    }  
    #box-colors {
      @include flexbox();
      flex-wrap: wrap;
      .changecolor {
        display: block;
        width: 20px;
        height: 20px;
        text-indent: -9999px;
        margin: 0 3px 3px 0;
        cursor: pointer;
        &.green {
          background-color: #a3d133;
        }
        &.pink {
          background-color: #ce1d76;
        }
        &.black {
          background-color: #000;
        }
        &.gold {
          background-color: #dab26d;
        }
        &.yellow {
          background-color: #fed700;
        }
        &.blue {
          background-color: #0787ea;
        }
        &.red {
          background-color: #f30b0b;
        }
        &.orange {
          background-color: #f89a20;
        }
        &.flat-blue {
          background-color: #00abc5;
        }
      }      
    }
    .box-menu-wrapper {
      &.opened {
        .box-menu {
          ul {
            visibility: visible;            
            @include opacity(1);
            @include translate(0, 0);
          }
        }
      }
    }
  }
}
#shopify-section-preview-panel {
  &.opened {
    .preview-panel {      
      width: 205px;      
      background: $white;
      border: 1px solid #ddd;
      border-right: none;     
      @include box-shadow(0 2px 9px 2px rgba(0,0,0,0.14));
    }
    #pp-wrapper {      
      @include translate(0, 0);
      @media (max-width: 767px) {
		max-height: calc(100vh - 100px);  
		overflow-x: hidden;
		overflow-y: auto;
      }
    }
    .pp-toggle {
      right: auto;
      left: -40px;
    }
  }  
}
