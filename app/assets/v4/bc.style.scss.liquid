          color: $body-general-color;
        }
        &:hover { 
          @media (min-width: 1025px) {
            background: $body-second-color;
            a {
              color: $body-title-color;            
            }
          }          
        }
        &.active {          
          background: $body-second-color;
          a {
            cursor: pointer;
            color: $body-title-color;            
          }
        }        
      }
    }
    &:hover {
      @media (min-width: 1025px) {
        #sort_by_box {
          /*@include dropdown-effect-hover;  
          @include translate(0, 0);*/
        }
      }
    }
    &.opened {
      #sort_by_box {
        @include dropdown-effect-hover;
        @include translate(0, 0);
      }
    }
  }
  .show-by {
    display: inline-block;
    margin: 0 0 10px 4%;
    position: relative;
    .show-by-button {
      border: none;
      padding: 6px 20px;
      line-height: 38px;
      background: $body-second-color;  
    }
    #show-by-box {
      width: auto;     
      min-width: 85px;
      background: $white;          
      padding: 10px 20px;
      display: block;  
      visibility: hidden;
      z-index: -99;
      list-style: none;
      text-align: left;
      @include ab(100%, 0, n, 0);
      @include translate(0, 10px);  
      @include opacity(0);
      @include  transition(all 0.35s ease);
      li{
        padding: 0;        
        margin-bottom: 0; 
        border-bottom: 1px solid $body-border-color;
        a {
          padding: 8px 0;
          line-height: 26px;
          display: block;
          transition: none;                               
          font-size: $body-font-size;
          color: $body-general-color;
        }
        &:hover {     
          @media (min-width: 1025px) {
            a{
              color: $main-color;            
            }
          }         
        }
        &.active { 
          > span {
            padding: 8px 0;
            line-height: 26px;
            display: block;           
            font-size: $body-font-size;
            color: $main-color; 
          }         
          a{
            cursor: default;
            color: $main-color;           
          }
        }
        &:last-child {
          border-bottom: none;          
        }
      }
    }
    &:hover,
    &.opened {
      #show-by-box {
        visibility: visible;
        z-index: 99;          
        @include opacity(1);  
        @include translate(0, 0);
      }
    }
  }
  .top-pagination-holder {
    float: right;
    margin: 5px 0;
    > span {
      float: left;
      line-height: 40px;
      padding: 0 7px 0 0;
    }  
    .pagination {
      float: left;
    } 
  }
  .visible-xs{
    cursor: pointer;               
  }
}
