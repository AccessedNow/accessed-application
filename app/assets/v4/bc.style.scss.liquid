              > .dropdown-menu {
                background: $nav-dropdown-background;                
                font-size: $body-font-size;
                padding: 10px 0;
                display: block !important;  
                left: 100%;
                top: 0;                  
                margin-left: 0;
                min-height: 372px;
                @include dropdown-effect;
                @include box-shadow(2px 2px 4px 1px rgba(0, 0, 0, 0.2));                  
                &:after {
                  content: '';
                  width: 17px;
                  @include ab(0, -15px, 0, n);
                }
                @media (max-width: 1199px) and (min-width: 992px) {
                  min-height: 332px;
                }                
                &.width-100 { 
                  width: 900px; 
                  @media (max-width: 1199px) and (min-width: 992px) {
                    width: 735px;
                  }
                }                
                &.width-75 { 
                  width: 675px; 
                  @media (max-width: 1199px) and (min-width: 992px) {
                    width: 550px;
                  }
                }
                &.width-50 { 
                  width: 450px; 
                  @media (max-width: 1199px) and (min-width: 992px) {
                    width: 370px;
                  }
                }                
                &.width-25 { 
                  width: 225px;
                  @media (max-width: 1199px) and (min-width: 992px) {
                    width: 185px;
                  } 
                }       
                > li {
                  > a {                  
                    color: $nav-dropdown-link-color;
                    white-space: normal;
                    span {
                      color: $nav-dropdown-link-color;
                      font-size: $body-font-size;
                    }               
                  }
                  span.expand {
                    line-height: 32px;
                    height: 32px;
                  }
                  a.dropdown-link {
                    padding-right: 0;
                    margin-right: 30px;
                  }
                  .dropdown-menu {
                    background: $nav-dropdown-background;         
                    > li {
                      > a {                      
                        color: $nav-dropdown-link-color;
                        white-space: normal;
                        span {
                          color: $nav-dropdown-link-color;
                        }
                      }                  
                      &:hover {  
                        > a {                      
                          span {                        
                            color: $nav-dropdown-link-hover-color;
                          } 
                        } 
                        > span.expand { 
                          color: $nav-dropdown-link-hover-color;                  
                        }
                      }
                    }
                  }
                  &:hover {  
                    > a {                    
                      span {
                        color: $nav-dropdown-link-hover-color;
                      } 
                    } 
                    > span.expand { 
                      color: $nav-dropdown-link-hover-color;                  
                    }
                  }
                }           
                .product-button {
                  display: none;
                }
                .bp-item {
                  .sb-product-head {
                    .product-name {
                      font-size: $product-font-size;
                      padding: 0;
                      margin: 0 0 15px;
                      border: none;
                      &:before,
                      &:after {
                        content: none;
                      }
                      > a {
                        color: $body-title-product;
                        font-size: $product-font-size;
                        font-weight: 700;                         
                        margin: 0;
                        display: block;
                        &:hover {
                          color: $nav-dropdown-link-hover-color;
                        }
                      }
                    }
                  }
                  .featured-img {
                    a {
                      padding: 0;
                    }
                  }
                  .sb-product-content {
                    padding: 0;
                  }
                  .bp-content-inner {
                    padding: 15px 0 0;                        
                    .sb-price {
                      padding: 6px 0 0;
                    }                   
                    .price-compare,
                    .price-sale {
                      display: inline-block;
                    }
                    .price-compare {                        
                      font-weight: 400;
                      font-size: 12px; 
                      margin-top: 5px;
                    }
                    .price, .price-sale {
                      font-size: $product-price-size;  
                      font-weight: 400;
                    }                     
                  }
                  &:hover {
                    background: none;
                    a {
                      text-decoration: none !important;
                    }
                  }
                }           
              }
              .dropdown-menu-sub {
                background: transparent;
                border: 0;
                border-radius: 0;
                box-shadow: none;
                position: static;
                width: 100%;
                min-width: auto;  
                margin: 0;           
                > li {
                  > a {                  
                    color: $nav-dropdown-link-color;
                    padding: 5px 0;
                    border-bottom: none;
                    span {
                      color: $nav-dropdown-link-color;
                    }
                    &:hover {                              
                      span {
                        color: $nav-dropdown-link-hover-color;
                      }
                    }
                  }
                  &:hover {
                    background: none;
                    > a {                  
                      text-decoration: underline;
                    }
                  }
                }
              }
              &:hover, &.hold {                         
                > .dropdown-menu {
                  @include dropdown-effect-hover;           
                }
              }
              &.mega-menu:hover > .dropdown-menu, 
              &.mega-menu.hold > .dropdown-menu {
                .dropdown-menu-sub {
                  display: block;
                }
              }
            }
            &:last-child {
              border-bottom: none;
            }                                  
          }
          > li.mega-menu {            
            > .dropdown-menu {
              padding: 20px;
              .row {
                margin-left: -15px;
                margin-right: -15px;
                width: auto;
              }
            }
            .dropdown-menu {
              .mega-sub-link {
                overflow: hidden; 
                margin: 0 0 20px;               
                > a {                                                
                  font-size: $nav-sub-font-size + 2px;  
                  font-weight: 700;
                  position: relative;
                  display: block;
                  padding: 5px 0;
                  margin: 0 0 5px;
                  line-height: 26px;                   
                  color: $nav-dropdown-link-color;
                  &:hover {
                    text-decoration: none;
                    color: $nav-dropdown-link-hover-color;                                   
                  }                 
                }
                > span.expand {
                  display: none;
                }
                .dropdown-menu-sub {
                  &:before {
                    content: none;
                  }
                  li {
                    padding: 0;                  
                    a {
                      &:hover {                      
                        color: $nav-dropdown-link-hover-color;                        
                      }
                    }
                    &.bp-item-2 {
                      padding-top: 15px;
                    }
                  }
                }
                &.no-title-0 {
                  > a,
                  > span.expand {
                    display: none !important;
                  }                               
                }
                &.no-title-2 {
                  > a,
                  > span.expand {
                    display: none !important;
                  }                        
                }
                &:last-child {
                  margin: 0;
                }  
              }
              .product-title {
                a {
                  color: $nav-dropdown-link-color !important;
                  &:hover{
                    color: $nav-dropdown-link-hover-color !important;
                  }
                }
              } 
              .menu-proudct-carousel {
                &.owl-theme .owl-controls .owl-nav {
                  position: static;
                  > div {
                    @include ab(50%, n, n, n);
                    @include translate(0, -50%);
                    &.owl-prev {
                      left: 0;
                    }
                    &.owl-next {
                      right: 0;
                    }
                  }
                }
              }
              .product-wrapper {
                background: none;
                padding: 0;
                &:after,
                &:before {
                  content: none;
                }
                .featured-img {
                  a {
                    padding: 0;
                  }
                }
                .product-name {
                  border: none;
                  padding: 0;
                  font-size: $product-font-size;
                  margin: 0 0 15px;
                  &:before {
                    content: none;
                  }
                  a {
                    padding: 0;    
                    font-size: $product-font-size;
                  }
                }
                .product-vendor {
                  display: none;
                  a {
                    padding: 0;
                    font-size: $body-font-size - 2px;
                    line-height: 20px;
                  }
                }
                .product-content .price-cart-wrapper {
                  border: none;
                  padding: 0;
                  min-height: auto;
                  .product-add-cart {
                    display: none;
                  }
                }
                .product-price {
                  line-height: 1.3;
                }
                .sb-price,
                .price {
                  color: $body-price-color;   
                  font-size: $product-price-size;  
                  font-weight: 400;
                }
                .price-sale {
                  color: $body-price-sale-color;     
                  font-size: $product-price-size;  
                  font-weight: 400;
                }
                .price-compare {
                  font-size: 12px; 
                  display: inline-block;
                  margin-top: 5px;
                }
              }             
            }          
          }
        }
      }      
    }
  }
}
.navbar {
  .bar {
    height: 3px;
    background: $nav-link-color;
    margin: 0 0 5px;
    display: block;
    @include border-radius(3px, 3px, 3px, 3px);
    &:nth-child(1) {
      width: 30px;
    }
    &:nth-child(2) {
      width: 24px;
    }
    &:nth-child(3) {
      width: 18px;
    }
