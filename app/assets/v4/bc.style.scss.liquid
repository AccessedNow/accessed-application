.cata-grid-1 {
  .cata-product {
    .product-grid-item {
      width: 100% !important;
      padding: 0 0 30px;
      margin: 0 0 30px;
      border-top: none !important;
      border-bottom: 1px solid $body-border-color;
      @media (max-width: 767px) {
        padding-bottom: 20px;
        margin-bottom: 20px;
      }
      .product-wrapper {
        float: none;
        width: 100%;               
        border: none;
        padding: 0;  
        @media (min-width: 768px) {
          @include flexbox();
        }
        &:before,
        &:after {
          content: none;
        }   
        .product-head {                    
          padding: 0 15px 0 0;
          margin: 0;    
          position: relative;            
          @media (min-width: 768px) {
            -webkit-box-flex: 0;        
            max-width: 75%;
            @include flex(0 0 75%);
          }        
          @media (max-width: 767px) {
            width: 100%;    
            display: block;   
            padding: 0 
          }                             
        }
        .featured-img {
          float: left;
          clear: none;
          margin: 0;
          padding: 0 15px;
          @media (min-width: 992px)  {
            width: 43.7%;            
          }
          @media (max-width: 991px) and (min-width: 768px) {
            width: 50%;
          }          
          @media (max-width: 767px) and (min-width: 576px) {
            width: 25%;
          }
          @media (max-width: 575px) {
            width: 43%;
          }
        }
        .product-image {      
          position: relative;
          overflow: hidden;
          padding: 0;
          max-width: 100%;
          @media (max-width: 767px) {
            overflow: visible;
          }
          a {
            &:hover {
              @media (min-width: 1025px) {
                &:before {
                  @include opacity(1);
                }
              }
            }
          }
          .product-group-vendor-name,
          .product-des-list {
            float: right;
            padding-left: 15px;
            @media (min-width: 992px) {
              width: 56.3%;              
            }              
            @media (max-width: 991px) and (min-width: 768px) {
              width: 50%;
            }              
            @media (max-width: 767px) and (min-width: 576px) {
              width: 75%;
            }
            @media (max-width: 575px) {
              width: 56%;
            }            
          } 
          .product-des-list {
            @media (min-width: 1200px) {
              display: block;
            }
            @media (max-width: 1199px) and (min-width: 768px) {
              display: -webkit-box;
              display: -ms-box;
              -webkit-line-clamp: 5;
              -webkit-box-orient: vertical;
              max-height: 105px;   
            }
            @media (max-width: 767px) {
              display: none;
            }
            ul {              
              li {
                list-style: circle outside none;
              }
            }
            ol {
              li {
                list-style: decimal outside none;
              }
            }
            ul, ol {
              padding-left: 18px;
              li {
                margin: 0 0 3px;
                white-space: normal;
              }
            }
          }
        }  
        .product-name {
          height: auto !important;
          @media (max-width: 479px) {
            margin-bottom: 10px;
          }
        }
        .product-content {
          text-align: left !important;          
          padding: 0 0 0 15px; 
          .product-description {
            display: none;
          }
          @media (min-width: 768px) {
            -webkit-box-flex: 0;        
            max-width: 25%;       
            @include flex(0 0 25%);
          }   
          @media (max-width: 991px) {
            padding-left: 5px;
          }
          @media (max-width: 767px) and (min-width: 576px) {
            padding: 0 0 0 15px;
            width: 75%;
            float: right;
          }
          @media (max-width: 575px) {
            padding: 0 0 0 15px;
            width: 56%;
            float: right;
          }   
          &:before {
            content: none;
          }       
          .product-name {
            font-size: $product-font-size + 6px;
            height: auto !important;
          }  
        }
        .price-cart-wrapper {
          padding-bottom: 10px;
          margin: 0 0 10px;
          border-bottom: 1px solid $body-border-color;
          @media (max-width: 1024px) {
            border-bottom: none;
            margin-bottom: 0;
          }         
          @media (max-width: 479px) {           
            margin: 0;
            padding: 0;
          } 
        }        
        .product-review {      
          min-height: auto;
          display: block;      
        }                          
        .product-button {
          @include opacity(1);
          @include translate(0, 0);
          position: static;
          border: none;
          background: none;
          padding: 0;    
          text-align: left;
          z-index: 1;
          margin: 0 -4px;          
        }           
      }
      &:last-child {
        border-bottom: none;
        padding-bottom: 0 !important;
        margin-bottom: 0 !important;
      }
    }
  }
}
