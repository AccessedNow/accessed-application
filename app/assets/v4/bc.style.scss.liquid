    z-index: 2;
    @include ab(n, 50%, 10px, n);
    @include translate(-50%, 0);
    @media (max-width: 1024px) {
      position: static;
      @include translate(0, 0);
    }
    .owl-dot {
      display: inline-block;
      margin: 0;
      span {
        display: block;
        width: 8px;
        height: 8px;
        border: none;
        background: $bg-other;
        margin: 0 7px;
        @include border-radius-circle;
        @include transition(all 0.35s ease 0s);
      }
      &.active {
        span {
          width: 30px;
          background: $main-color;
          @include border-radius(4px, 4px, 4px, 4px);          
        }
      }
      &:hover {
        @media (min-width: 1025px) {
          span {
            width: 30px;
            background: $main-color;
            @include border-radius(4px, 4px, 4px, 4px);          
          }
        }
      }
    }
  }
  .owl-controls {
    .owl-nav {                 
      @include ab(-70px, n, n, 0);       
      > div {          
        @include transition(all 0.35s ease 0s);            
        display: inline-block;
        padding: 0;
        margin: 0; 
        height: 30px;     
        width: 20px;  
        z-index: 3;        
        span {
          display: inline-block; 
          height: 100%;
          width: 100%;   
          cursor: pointer;
          text-align: center;
          line-height: 30px; 
          color: $body-light-color;         
          &:before {        
            font-size: $font-small-icon;            
            font-family: arenafont;      
          }
        }
        &.owl-next {      
          @media (max-width: 1024px) {
            right: 0; 
          }   
          span {
            &:before {
              content: "\f105";                    
            }
          } 
        }
        &.owl-prev {
          @media (max-width: 1024px) {
            left: 0;
          }
          span {
            &:before {
              content: "\f104";
            } 
          }
        }
        &:hover {
          @media (min-width: 1025px) {        
            span {
              color: $body-title-color;
              &:before {
                color: $body-title-color;
              }
            }
          }
        }
      }
    }
  }  
}

