/*Popup Cart*/
#layer-addcart-modal {
  z-index: 99999;    
  .addcart-overlay {
    background: rgba(0, 0, 0, 0.5);   
    height: 100%;
    left: 0;    
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 99992;    
  }
  .addcart-modal-close {
    @include border-radius-circle;    
    @include box-shadow(0 0 0 rgba($black, 0.5));
    @include ab(-25px, n, n, -20px); 
    @include transition(all 0.35s ease 0s);                    
    @include width-btn-1;          
    display: block;
    cursor: pointer;  
    text-align: center;
    z-index: 9999;     
    padding: 0;
    margin: 0;  
    @media (max-width: 767px) {
      top: 5%;
      right: 5%;  
      width: 35px;
      height: 35px;   
      line-height: 35px;
      position: fixed;  
    }
    i {
      @include close-btn;   
      color: $white;
      @media (max-width: 767px) {
        line-height: 35px;
      }
    }
    &:hover {
      background: $button-hover-btn1;
      i {     
        color: $button-hover-btn1-txt;
      }
    }
  }
  .addcart-modal-box {
    @include border-radius-20;
    @include ab(15%, n, n, n);
    @include box-shadow(0 0 7px 5px rgba(0, 0, 0, 0.3));
    padding: 30px;
    margin: 0 calc((100% - 680px) / 2);
    width: 680px;
    height: auto;
    max-height: 90%;    
    z-index: 99999;
    background: $body-background-color;    
    @media (max-width: 767px) {
      top: 5%;
      width: 90%;
      margin: 0 5%;
      overflow-y: auto;
    }
    @media (max-width: 479px) {
      padding: 20px;
    }
    p {
      margin: 0 5px 0 0;      
      color: inherit;     
      line-height: 30px;
      display: inline-block; 
      @media (max-width: 767px) {
        margin-bottom: 20px;
      }
      a {
        color: inherit;
        &:hover {
          color: inherit;
          text-decoration: underline;
        }
      }      
    }
    .am-close {
      display: inline-block;
      cursor: pointer;     
      i {
        margin-right: 5px;
        display: inline-block;
        &:before {
          @include close-btn;
          color: inherit; 
          line-height: 25px; 
        }
        &:hover {
          &:before {
            color: inherit;
          }
        }
      }
    }
  }
  .addcart-modal-content {    
    .note {
      color: $popup-letter-subtitle;
      line-height: 20px;
      margin: 0 0 30px;
      @media (max-width: 767px) {
        padding-right: 20px;
        margin-bottom: 20px;
      }
    }
  }  
  .addcart-modal-product {
    overflow: hidden;
    padding: 0 0 20px;
    .addcart-modal-image {
      float: left;
      max-width: 100px;
      margin: 0 30px 0 0;
      @media (max-width: 479px) {
        margin-right: 20px;
      }
    }
    .addcart-modal-info {
      margin-left: 130px;
      @media (max-width: 479px) {
        margin-left: 120px;
      }
      strong {
        margin: 0 10px 0 0;
      }
      > div {
        margin: 0 0 8px;
      }
      .addcart-modal-title {
        font-weight: 700;
        color: $body-title-product;
        font-size: $body-font-size + 2px;
      }
      .addcart-modal-qty {
        strong {
          font-weight: normal;
        }
      }
      .addcart-modal-price {                       
        font-size: 18px;
        strong {
          color: $body-general-color;
          font-weight: 400;
        }
      }
    } 
  }
  .addcart-modal-action {
    @include clearfix();
    padding: 20px 0 10px;
    border-top: 1px solid $body-border-color;
    .addcart-modal-number {
      font-size: $body-font-size + 2px;
      margin: 0 0 10px;
      float: left;
      @media (min-width: 560px) {
        margin-top: 10px;
      }
      @media (max-width: 559px) {
        margin-bottom: 5px;
      }      
    }
    .addcart-modal-subtotal {               
      font-size: $size-h2;
      float: right;
      margin: 0 0 10px;            
      align-items: center;
      -webkit-align-items: center;      
      @include flexbox();
      @media (max-width: 559px) {
        margin-bottom: 5px;
        clear: both;
        float: left;
      }     
      strong {
        margin: 0 20px 0 0;
        color: $body-general-color;
        font-weight: 400;
        font-size: $size-h4;
      }
    }
    .wrap-progress {
      padding: 20px 0 0;
      clear: both;      
    }
    .addcart-modal-button {
      overflow: hidden;
      clear: both;        
      .addcart-modal-continue {
        float: left;          
        border: none;
        margin: 10px 0 0;    
        @media (max-width: 570px) {
          .btn {
            min-width: 205px;    
          } 
        }          
      }
      .addcart-modal-checkout {
        float: right; 
        margin: 10px 0 0;    
        @media (max-width: 530px) {
          clear: both;
          float: left;
        }
      }
    }
  }
