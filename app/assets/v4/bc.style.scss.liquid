      overflow: hidden;
      position: relative;  
      display: table;
      width: 100%;
      table-layout: fixed;  
      margin: 0;
      padding: 15px 0;
      border-bottom: 1px solid $body-border-color;
      .cart-item-image {
        float: none;
        display: table-cell;
        vertical-align: top;
        width: 85px;        
        padding: 0 15px 0 0; 
        text-align: center;   
