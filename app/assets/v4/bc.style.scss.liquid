        text-transform: capitalize;
        cursor: pointer;
        font-weight: 400;
      }   
      span {                
        cursor: pointer;        
        i {
          display: none;
          &:before {
            content: none;
          }         
        } 
      }
    }    
  } 
  .sb-filter-tag {
    overflow: auto;
    /*max-height: 400px;*/
    li {
      margin: 0 0 13px !important;
      line-height: 20px;
      a {
        position: relative;
        padding-left: 25px;
        min-height: 20px;
        display: inline-block;   
        font-size: $body-font-size;   
        word-break: break-word;  
        &:before{
          content: "";
          @include ab(3px,0,n,n);          
          width: 16px;
          height: 16px;              
          border: 1px solid $body-border-color;  
          @include border-radius(3px, 3px, 3px, 3px);
          @include transition(all 0.35s ease 0s);            
        }
        &:after {
          content: "\e8ca";
          font-family: arenafont;
          font-size: 12px;
          width: 16px;
          height: 16px; 
          @include ab(1px, 2px, n, n);              
          @include opacity(0);
          @include transition(all 0.35s ease 0s);              
        }
        &:hover {
          @media (min-width: 1025px) {
            &:before {
              border-color: $main-color;
            }            
          }              
        }
      }
      &.active {
        > span {
          position: relative;
          padding-left: 25px;
          min-height: 20px;
          display: inline-block;          
          color: $body-title-color;
          font-size: $body-font-size;
          font-weight: 700;
          &:before{
            content: "";
            @include ab(3px,0,n,n);            
            width: 16px;
            height: 16px;              
            border: 1px solid $body-title-color;  
            @include border-radius(3px, 3px, 3px, 3px);
            @include transition(all 0.35s ease 0s)            
              }
          &:after {
            content: "\e8ca";
            font-family: arenafont;
            font-size: 12px;
            width: 16px;
            height: 16px; 
            @include ab(1px, 2px, n, n);              
            @include opacity(1);
            @include transition(all 0.35s ease 0s);              
          }
        }
      }
    }
  }
