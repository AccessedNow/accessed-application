/* header ----------------*/
.top-bar {    
  border-bottom: 1px solid rgba($text-header-color, 0.1);
  .table-row {
    display: table;
    width: 100%; 
    max-width: 1400px;  
    margin: 0 auto; 
    padding: 4px 0;   
    .list-inline {
      > li {
        line-height: 28px;
        font-size: $top-font-size;
        padding: 0 15px;
        position: relative;
        color: $text-header-color;
        &:before {
          content: '';
          height: 15px;
          width: 1px;
          background: rgba($text-header-color, 0.1);
          @include ab(7px, n, n, 0);
        }
        > i {          
          margin: 0 5px 0 0;          
          font-size: $body-font-size;
        }
        > img {
          display: inline-block;
          margin: 0 5px 0 0;    
          vertical-align: middle;      
        }
        > a {
          color: $text-header-color;
          &.waiting {
            padding: 0 !important;
            background: transparent !important;
          }
          > i {            
            margin: 0 7px 0 0;            
          }
          > img {
            display: inline-block;
            vertical-align: middle;
            margin: 0 5px 0 0;          
          }
          &:hover {
            color: $main-color;
          }
        }
        .customer-or {
          padding: 0 3px;
          color: rgba($text-header-color, 0.6);
        }
        &.waiting {
          padding-top: 0 !important;
          background: transparent !important;
        }
        &.email,
        &.phone {
          .demo-icon {
            color: $main-color;
          }
        }
        &:first-child {
          padding-left: 0;          
        }
        &:last-child {
          padding-right: 0;
          &:before {
            content: none;
          }
        }
        &:hover {
          > i {
            color: $main-color;
          }
        }
      }
    } 
    > div {
      display: table-cell;
      vertical-align: middle;      
    }
    .top-bar-right {
      text-align: right;
    }
    .currency {
      position: relative;
      .currencies {
        @include ab(100%, 0, n, n);
        width: auto;
        min-width: 65px;
        padding: 5px 0;
        margin: 4px 0 0;
        text-align: left;
        list-style: none;
        background: $white;
        border: 1px solid $body-border-color;
        @include border-radius-reset;
        @include box-shadow(0 6px 12px rgba($black, 0.176));
        visibility: hidden;
        z-index: -99;       
        @include translate(0, 20px);  
        @include opacity(0);
        @include transition(all 0.35s ease 0s); 
        &:before {
          content: '';
          height: 2px;
          background: $main-color;
          @include ab(-1px, -1px, n, -1px);
        } 
        &:after {
          content: '';
          height: 6px;
          @include ab(-5px, 0, n, 0);
        }
        > li {
          display: block;
          padding: 1px 15px;
          > a {
            display: block;
            white-space: nowrap;
            line-height: 1.42857143;
            color: $body-general-color;
            i {
              display: inline-block;
              margin-right: 5px;
            }
            span {
              font-size: $body-font-size - 2px;
            }
            &:hover {
              color: $main-color;
            }
          }
        }
      }
      .dropdown-toggle {
        &:after {
          content: none;
        }
      }
      &.opened {
        .currencies {
          @include opacity(1);
          @include translate(0, 0);
          visibility: visible;
          z-index: 99;
        }
      }
    }
  }
}
