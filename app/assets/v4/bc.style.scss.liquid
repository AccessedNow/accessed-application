  @include ab(100%, 0, n, n);
  @include border-radius-reset;  
  @include box-shadow(0 6px 12px rgba($black, 0.176));  
  float: none;
  text-align: left;
  display: none;  
  width: 245px;
  z-index: 999;
  padding: 0;
  margin: 0;
  border: 1px solid $body-border-color;
  background: $body-background-color;  
  @media (max-width: 1199px) and (min-width: 992px) {
    width: 235px;
  }
  &:before {
    content: '';
    height: 2px;
    background: $main-color;
    @include ab(-1px, -1px, n, -1px);
  } 
  > li {
    > a {
      padding: 3px 20px;      
      line-height: 1.42857143;
      &:hover,
      &:focus {
        background-color: transparent; 
        color: $nav-dropdown-link-color; 
      }
    }
  }
  > .active {
    > a {
      background-color: transparent; 
      color: $nav-dropdown-link-color;
      &:hover,
      &:focus {
        background-color: transparent; 
        color: $nav-dropdown-link-color;
      }
    }
  }
}
.main-nav {
  > li {  
    position: relative;  
    &.mega-menu {
      position: static;
    }
  }
  .mega-menu .dropdown-menu-sub {
    position: static;
    padding: 0 0 10px;
  }
  .dropdown-menu {
    padding: 15px 0;
    width: 270px;    
    li{
      padding: 0 20px;      
      a { 
        white-space: normal;        
        padding: 3px 0;
        margin: 0;
        line-height: 26px;                 
        position: relative;  
        text-transform: none;  
        font-size: $nav-sub-font-size;       
      }
    }
  } 
}
