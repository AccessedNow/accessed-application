        min-width: 100%;
        min-height: 100%;
        @include ab(50%, 50%, n, n);
        @include translate(-50%, -50%);
      }
      &:hover {
        @media (min-width: 1025px) {
          .swiper-content { 
            z-index: 4;
          }    
        }      
      }
    }
    &.swiper-slide-active {
      .container-box {
        @include opacity(1);
      }    
    }
    > a  {
      display: block;
      width: 100%;
      height: 100%;
      position: relative;
      z-index: 1;
      padding: 0;
      &.waiting,
      &.lazyloaded {
        height: 100%;
      }
      > img {        
        @include opacity(0);
        position: static;
        &.lazyloaded,
        &.waiting {
          @include opacity(0);
          position: static;
        }
      } 
    }     
  }  
  .swiper-content {           
    margin: 0 auto;          
    @include ab(10%, 0, n, 0);  
    @media (max-width: 991px) {
      padding-left: 15px;
      padding-right: 15px;
    }
    @media (max-width: 767px) {
      top: 50% !important;
      @include translate(0, -50%);
      display: table;
    }
    &.fix-textbox {
      .container-box {
        @media (min-width: 1200px) {
          padding-left: calc(24.3% + 35px);             
        }
        @media (max-width: 1199px) and (min-width: 992px) { 
          padding-left: calc(24.1% + 15px);           
        }       
      }
    }    
  }
  &.flexslider {
    border: 0;
    margin-bottom: 0;
    @include border-radius-reset;
    .flex-direction-nav {
      a{        
        z-index:4; 
        display: block; 
        height: 40px; 
        width: 40px;        
        border: none;
        text-align: center;         
        background: rgba(0, 0, 0, 0.3);        
        @include border-radius-reset; 
        @include ab(45%, n, n, n);
        &:hover {
          @include opacity(1);
          background: rgba(0, 0 ,0 , 0.5);
        }
      }
      a.flex-prev {
        left: 10px;
        &:before{
          content: "\f104";
        }
      }
      a.flex-next {
        right: 10px;
        &:before{
          content: "\f105";
        }       
      }       
    }
    .flex-control-nav {
      bottom: 4%;
      z-index: 10;
    }
    .slides > li {
      position: relative;     
    }
  }
  .cameracurrent , .flex-active-slide {
    .camera_caption > div{
      @include opacity(1);
      -webkit-transition: all 0.5s ease 0s;
      transition: all 0.5s ease 0s;
    }
  }
  .flex-control-paging li {
    a {
      width: 12px;
      height: 12px; 
      margin: 0;     
      background-color: $white;
      border: 2px solid #333; 
      background: #333;
      @include border-radius(50%, 50% , 50%, 50%);
      position: relative;
      padding: 0; 
      @include opacity(0.6);
      &:hover, &.flex-active {
        border-color: $main-color; 
        background: $main-color;      
      }
    }
  }  
}
.swiper-button-white {  
  z-index:4; 
  display: block; 
  height: 60px; 
