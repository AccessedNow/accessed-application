/* Menu mobile -----------------------------*/

.mobile-version {
  width: 100%; 
}
.mobile-layout-bar {
  border-top: 1px solid rgba($main-color-text, 0.1);
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 9999;
  max-height: 52px;
  background: $main-color;
  .waiting {
    background: transparent;
    padding: 0;    
  }
  img {
    image-rendering: pixelated;
    &.lazyloaded {
      width: auto !important;
    }
  }
  .m-block-icons {
    display: table;
    width: 100%;
    table-layout: fixed;
    > li {
      display: table-cell;
      vertical-align: middle;
      text-align: center;
      border-right: 1px solid rgba($main-color-text, 0.1);
      > a {
        color: $main-color-text;
        display: block;        
        line-height: 50px;         
        position: relative;  
        &.waiting {
          height: 100%;
        }
        i {
          font-size: $font-small-icon;
        }   
        .expand {
          i {
            font-size: 14px;
          }
        }
      }
      &:last-child {
        border: none;
      }
      &.wishlist-target,
      &.compare-target {
        .wishlist-icon,
        .compare-icon {
          .number {
            @include icons-number;                        
            @include translate(-50%, 0);   
            background: $text-header-color;
            color: $bg-header-color;
            top: auto;
            right: auto;   
            left: 50%;
            bottom: 12px;
            margin-left: 12px;      
          }
        }
      }
      &.currency {
        position: relative;
        .dropdown-toggle {
          &:after {
            content: none;
          }
        }
        .currencies {
          @include ab(n, 50%, 55px, n);
          @include translate(-50%, 0);
          width: auto;         
          min-width: 65px;
          padding: 5px 0;
          margin: 0;
          text-align: left;
          list-style: none;
          background: $white;          
          border: 1px solid $body-border-color;          
          visibility: hidden;
          z-index: -99;                  
          @include opacity(0);
          @include border-radius-reset;
          @include box-shadow(0 6px 12px rgba($black, 0.176));
          @include transition(all 0.35s ease 0s); 
          &:before {
            content: '';
            height: 2px;
            background: $main-color;
            @include ab(-1px, -1px, n, -1px);
          } 
          > li {
            display: block;
            padding: 0 15px;
            > a {
              display: block;
              white-space: nowrap;
              i {
                display: inline-block;
                margin-right: 5px;
              }
              span {
                font-size: $body-font-size - 2px;
              }
            }
          }
        } 
        &.opened {
          .currencies {
            @include opacity(1);
            visibility: visible;
            z-index: 99;
          }
        }            
      }      
    }
  }
}
