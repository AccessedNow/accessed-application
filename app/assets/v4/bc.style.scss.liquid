/* main slideshow -----------------------------------*/
.slideshow-catalog-wrapper {
  @include translate3d(0, 0, 0);
}
.bc-wrapper {
  position: relative; 
  z-index: 1;   
  overflow: hidden;   
  &:hover {
    .swiper-button-white {
      @include opacity(1 !important); 
      &.swiper-button-disabled {
        @include opacity(0 !important);
      }
    }
    .video-slide {
      .video {
        z-index: 1;
      }
    }      
  }
  &.full-height {
    .home-slideshow {      
      .swiper-slide {
        min-height: 100vh;
        > a {
          @include ab(0, 0, 0, 0);
        }
        @media (max-width: 1200px) {
          min-height: 80vh;
        }
        @media (max-width: 479px) {
          height: 60vh;
        }
      }
    }
  }
  &.full-padding {
    .container {
      max-width: 1430px;
    }
    .tab-wrapper {
      max-width: 1430px;
    }  
    .swiper-pagination {
      bottom: 18px;
      @media (max-width: 479px) {
        bottom: 0;
      }
    }	  	
    .swiper-slide {
      @media(min-width: 992px) {
        min-height: 700px;
      }
    }
  }
  &.boxed {
    margin: 0 auto;
    padding: 0 15px; 
    @media (min-width: 767px) {
      width: 100%;
    }  
    @media (min-width: 768px) {
      max-width: 760px;
    }
    @media (min-width: 992px) {
      max-width: 980px;
    }
    @media (min-width: 1200px) {
      max-width: 1200px;
    }
    .swiper-button-next {
      @media (min-width: 768px) {
        right: 25px;
      }
    }
    .swiper-button-prev {
      @media (min-width: 768px) {
        left: 25px;
      }
    }
    .container {
      max-width: 100%;
      @media (min-width: 768px) {
        padding: 0 65px;
      }
    } 
  }
  .tab-wrapper {
    max-width: 1200px;
    padding: 0 15px;
    @media (min-width: 992px) {  		
      @include ab(n, 0, 0, 0);	  	
      margin: 0 auto;
    }	
    @media (max-width: 991px) {
      margin-top: 15px;
      display: none;
    }	
    .tab-content {
      position: relative;
      clear: both;
      z-index: 2;
      .tab-pane {
        @include ab(0, 0, n, n);
        @include opacity(0);
        width: 100%;
        &.active {
          position: static;
          @include opacity(1);
        }
      }
      .owl-stage {	  		
        padding-bottom: 48px;
        padding-top: 2px;	  		
      }
      .product-wrapper {
        &:before {
          content: none;
        }
        &:after {
          bottom: 0;
        }
        .product-head .product-name {
          height: 38px;
          line-height: 19px;
          overflow: hidden;
          display: -webkit-box;
          display: -ms-box;
          -webkit-line-clamp: 2;
          -webkit-box-orient: vertical;
          margin: 0 0 10px;
        }
        .product-content {
          .pc-inner .price-cart-wrapper {
            border: none;
            padding-bottom: 0;
          }
          .product-price {
            @media (min-width: 1025px) {
              @include ab(0, 0, n, n);
              width: calc(100% - 38px);			  			
            }
          }
        }
        &:hover {
          .product-content .product-button {
            display: none;
          }	  			
        }
      }
      .owl-theme .owl-controls .owl-nav {
        position: static;
        > div {
          @include ab(50%, n, n, n);
          @include translate(0, -50%);
          span {
            &:before {
              font-size: 40px;
            }
          }
          &.owl-prev {
            left: 0;
            @media (min-width: 1500px) {
              left: -38px;
            }
          }
          &.owl-next {
            right: 0;
            @media (min-width: 1500px) {
              right: -38px;
            }
          }
        }
      }
    }
  }
  .nav-tab-wrapper {  
    z-index: 2;	  	  		
    @media (min-width: 992px) {  		
      width: 200px;
      background: $body-background-color;
      @include border-radius(5px, 5px, 5px, 5px);
      float: right;
      margin: 0 0 3%;	  	
    }
    .nav {  		
      @media (min-width: 992px) {
        border: none;
        padding: 0;
        &:before {
          content: none;
        }
      }
      @media (max-width: 991px) {
        text-align: center;
      }
    }

    .nav-item {
      @media (min-width: 992px) {
        display: block;
        margin: 0;   		 		
        a {
          display: block;
          text-align: left;
          line-height: 22px;
          padding: 7px 25px;  			
          font-size: $body-font-size;  			
          &:before {
            content: none;
          }
          &:after {
            content: '';
            width: 26px;
            height: 26px;
            background: transparent;
            @include rotate(45);
            @include ab(5px, -13px, n, n);
            bottom: auto;
            right: auto;
            @include transition(all 0.35s ease 0s);
          }
          &:hover {
            @media (min-width: 1025px) {  					
              font-weight: $font-bold;	  				
            }
          }
          &.active {
            background: $main-color;
            color: $main-color-text;
            &:after {
              background: $main-color;
            }
          }
        }
        &:first-child {
          a {
            &.active {
              @include border-radius(0, 5px, 0, 0);
            }
          }
        }  		
        &:last-child {
          a {
            &.active {
              @include border-radius(0, 0, 5px, 0);
            }
          }
        }  		
      }
      @media (max-width: 991px) {
        padding: 0 20px;
      }
    }  	
  }  
}
