.product-wrapper {
  position: relative;  
  margin: 0; 
  padding: 15px 20px;  
  background: $body-background-color;
  @include transition(all 0.35s ease 0s);
  &:before {
    content: '';
    width: 1px; 
    height: 70%;  
    background: $body-border-color;
    @include ab(50%, 0, n, n);
    @include translate(0, -50%);
  }
  @media (min-width: 1025px) {
    &:after {
      content: '';
      width: 100%;
      display: block;
      z-index: -1;
      border: 1px solid $body-border-color;
      background: $body-background-color;
      @include border-radius(5px, 5px, 5px, 5px);
      @include ab(0, 0, -35px, 0); /*-9%*/
      @include box-shadow(0 0 5px 0 #c6c6c6); 
      @include transition(all 0.35s ease 0s); 
      @include rotateX(45deg);
      @include transform-origin(bottom);
      @include opacity(0);
    }  
  } 
  @media (max-width: 1199px) {
    padding-left: 15px;
    padding-right: 15px;
  }
  @media (max-width: 479px) {
    padding: 10px;
  }
  .product-review {
    display: none;
  }
  .product-head {
    .product-name {            
      color: $body-title-product;     
      font-size: $product-font-size;      
      margin: 0 0 15px;      
      line-height: 1.4;    
      position: relative;  
      font-weight: 700;     
      {% if settings.enable_title_blance == true %} 
      @media (max-width: 767px) {
        height: auto !important;
      }
      {% endif %}
      @media (max-width: 479px) {
        font-size: $product-font-size - 2px;      
      }
      a {
        color: inherit;
        font-size: inherit;      
      }
      &:hover a {        
        color: $main-color;
      }
    }
    .product-image {
      position: relative; 
      margin: 0 auto;
      padding: 0;       
      .featured-img {
        overflow: hidden;
        position: relative;        
        text-align: center; 
        width: 100%;
        margin: 0 auto 15px;
        {% if settings.enable_image_blance == true %}           
        a {     
          padding-top:  $image-height * 1%;                                 
          img {                                    
            @include ab(50%, 50%, n, n);                                                           
            @include translate3d(-50%, -50%, 0);         
          }          
        }
        {% endif %}
        a {
          display: block;    
          position: relative;            
          > span.img-back {
            img {
              @include opacity(0);
              @include ab(50%, 50%, n, n);                                                           
              @include translate3d(-50%, -50%, 0);              
            }
          }
          > img {
            &.lazyloading {
              filter: blur(5px); 
              -webkit-filter: blur(5px); /* Prefixed CSS3 blur filters */
              -moz-filter: blur(5px); 
              -o-filter: blur(5px); 
              -ms-filter: blur(5px);
              @include opacity(0.2);              
            }
            &.lazyloaded {              
              filter: blur(5px);
              -webkit-filter: blur(0px); /* Prefixed CSS3 blur filters */
              -moz-filter: blur(0px); 
              -o-filter: blur(0px); 
              -ms-filter: blur(0px);
              @include opacity(1);
            }
          }
        }
        img {            
          max-width: 100%;
          height: auto;  
          max-height: 100%;            
          margin: 0 auto;                    
          display:inline-block;
          vertical-align: middle;  
          @include transition(all 0.5s ease 0s);          
          &.lazyloaded {
            height: auto !important;     
            width: auto !important;  
          }
        } 
        &.switch-image {
          a {
            &:before {
              content: none;
            } 
            img {              
              @include preserve-3d; 
            } 
          }
          &:hover {
            @media (min-width: 1025px) {
              a {                         
                > img {
                  @include opacity(0);
                }
                > span.img-back {
                  img {
                    @include opacity(1);                                                  
                  }
                }            
              }
            }
          }         
        }
      }       
      .product-des-list {
        font-size: $body-font-size - 1px;   
        color: $body-light-color;           
        line-height: 18px;
        margin: 5px 0 0 0;
        white-space: nowrap;
        text-overflow: ellipsis;
        overflow: hidden;
        display: none;
      }
    }       
  }
  .product-vendor {   
    margin: 0 0 4px;
    line-height: 20px;    
    color: $body-light-color;
    font-size: $body-font-size - 2px;
    a {
      font-size: $body-font-size - 2px;
      color: $body-light-color;
      &:hover {
        color: $main-color;
      }
    }
  }
  .product-review {        
    margin: 0;  
    .spr-badge-starrating {
      float: none;
      margin: 0;
    }
    .spr-badge {
      margin: -3px auto 8px;
    }
  }         
  .product-content {                    
    .pc-inner {
      display: block;
      width: 100%;    
      @media (max-width: 1024px) {
        position: relative;
      }
    }
    .product-description {               
      margin: 0 0 15px;      
      display: none;   
      font-size: $body-font-size - 1px;
      color: $body-light-color;
      br {
        content: 'A';
        display: block;
        margin: 0 0 5px;
      }     
      ul {
        li {
          list-style: circle outside none;
        }
      }
      ol {
        li {
          list-style: decimal outside none;
        }
      }
