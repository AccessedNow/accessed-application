.panel-group {
  margin: 0; padding: 0;
  clear: both;
  .panel{
    border-radius: 0;
    border: 0;
    margin: 0;
    box-shadow: none;
    background: none;
    padding: 0;
    .collapse  {
      border: none;
    }
    .panel-heading {
      background: none;           
      text-transform: uppercase;
      padding: 0;
      border-bottom: 1px solid $body-border-color;

      .panel-title {
        color: $body-title-color;
        font-size: $size-h4;
        font-family: $font-heading;
        padding: 7px 0;
        a{
          color: $body-title-color;
          font-size: $size-h4;
          font-family: $font-heading;
          text-transform: uppercase;
          display: block;                           
          position: relative; 
          padding-left: 45px;  
          line-height: 30px;          
          @include transition(all 0.35s ease 0s);           
          &.collapsed {
            color: $body-title-color;             
          }
          &:hover{            
            color: $body-title-color;             
          }         
        }
      }
    }
    .panel-collapse {
      > .panel-body, > .list-group{
        border: none;
        border-bottom: 1px solid $body-border-color;
        padding: 25px 0;
        background: none;
      }
    }
    + .panel{
      margin-top: 0;
    }

    .panel-body {   
      font-size: $body-font-size;
      line-height: 24px;      
      iframe{
        max-width: 100%;
      }
      ul {
        li {
          padding: 1px 0;
        }
      }
    }
    &:last-child {
      padding-bottom: 0;
      border-bottom: none;
      .panel-heading {
        border-bottom: none;
      }
      .panel-collapse {
        > .panel-body, > .list-group {
          border-top: 1px solid $body-border-color;
          border-bottom: none;
        }
      }      
    }
  }
}

