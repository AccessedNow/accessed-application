      .dropdown-menu {        
        > .row { margin: 0; position: static;}       
        .mega-sub-link {                   
          overflow: hidden;
          &:not(:last-child) {
            border-bottom: 1px solid $body-border-color;
          }
          &.no-title-0,
          &.no-title-1,
          &.no-title-2 {
            > .dropdown-inner,            
           > a,
           > span.expand {
             display: none !important;
           } 
            > span.expand {
              margin: 0;
            } 
            .dropdown-menu {
              @include translate3d(0, 0, 0);     
              position: static;
              min-height: auto !important;
              max-height: none;
              .back-prev-menu {
                display: none !important;
              } 
