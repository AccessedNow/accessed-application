        &:before {
          content: "\e8cc";
          font-family: arenafont;
          font-size: 12px;
          margin-right: 7px;
        }
        &:hover {
          &:before {
            content: '\e8cc';       
          }
        }
      }
    }
  }
}
.filter-order-form {
  @include drawer-sidebar;
}
.filter-sidebar { 
  &.position-sidebar {
    &.style-normal {
      .sb-filter-wrapper {
        .f-close {
          @media (min-width: 992px) {
            display: none;
          }
        }
      }
    }
    &.style-accordion {
      .sb-filter-wrapper {
        .accordion {
          .sbf-title {
            position: relative;
            cursor: pointer;
            padding: 3px 20px 3px 0;            
            &:before {
              content: '\f106';
              font-family: arenafont;
              font-size: 14px;
              width: 20px;
              height: 20px;
              line-height: 20px;
              text-align: center;
              @include ab(2px, n, n, 0);
              @include transition(all 0.35s ease 0s);
            }            
          }
          &.accordion-active {           
            .sbf-title {            
              &:before {
                @include rotate(180);
              }
            }              
          }
        }        
        .f-close {
          @media (min-width: 992px) {
            display: none;
          }
        }
      }
    }
  }
  &.position-body {
    &.style-toggle {
      @include toggle-sidebar; 
      .sbw-filter > div {       
        .sb-filter {
          width: 25%;         
          @media (max-width: 767px) and (min-width: 480px) {
            width: 50%;
          }
          @media (max-width: 479px) {
            width: 100%;
          }
        }
      }
      .sb-filter-tag {
        padding-bottom: 10px;
        letter-spacing: -0.31em;
        vertical-align: top;
        li {
          padding: 0 10px 0 0;
          display: inline-block;
          letter-spacing: 0;
          vertical-align: top;
          @media (min-width: 1200px) {
            width:16.666667%;
          }
          @media (max-width: 1199px) and (min-width: 992px) {
            width: 20%;
          }
          @media (max-width: 991px) and (min-width: 768px) {
            width: 25%;
          }
          @media (max-width: 767px) and (min-width: 556px) {
            width: 33.333333%;
          }
          @media (max-width: 555px) {
            width: 50%;
          }
          &:first-child {
            display: block;
            width: 100%;
          }
        }
      }
    }
    &.style-drawer {
      @include drawer-sidebar;
      h5 {
        font-size: $size-h4;      
        font-weight: 400;     
        text-transform: capitalize; 
        border-bottom: 1px solid $body-border-color;
        line-height: 1.4;
        margin: 0 0 35px;
        padding: 0 0 10px;
        position: relative;
        &:before {
          content: '';
          width: 70px;
          height: 2px;
          background: $main-color;
          @include ab(n, 0, -1px, n);
        }
      }
      .sbw-filter > div .sb-filter:last-child {
        border-bottom: none;
        padding-bottom: 0;
      }
    }
  }
}
