    #sort_by_box {      
      width: auto;
      min-width: 175px;      
      background: $white;           
      padding: 0;      
      list-style: none;
      border: 1px solid $body-border-color;
      @include dropdown-effect;
      @include ab(100%, -1px, n, -1px);
      @include translate(0, 10px);         
      @include box-shadow(0 6px 12px rgba($black, 0.176));    
      &:before {
        content: '';
        height: 2px;
        background: $main-color;
        @include ab(-1px, -1px, n, -1px);      
      } 
      @media (max-width: 767px) {
        left: auto;
        right: 0;
      }
