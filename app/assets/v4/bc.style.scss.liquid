/* Back to top ---------------------------------*/
#scroll-to-top {
  @include transition(all 0.2s ease-out 0s); 
  -moz-backface-visibility: hidden;
  opacity: 0;
  filter: alpha(opacity=0);
  position: fixed;
  right: 15px;
  bottom: 10px;
  z-index: 1000;
  display: block;
  cursor: pointer;
  text-align: center;
  @media (max-width: 1024px) { 
    display: none;
  }
  &.on {
    bottom: 10%;
    opacity: 1;    
    filter: alpha(opacity=100); 
  }
  i {   
    display: none;    
  }
  a {
    position: relative;
    width: 45px;
    height: 45px;
    line-height: 45px;    
    display: block;
    border: none;
    @include transparent($main-color, 1);
    @include border-radius-circle;
    @include transition(all 0.3s ease-in-out 0s);    
    &:before {
      content: "\f106";
      font-size: 20px;
      padding: 0;
      width: 45px;
      height: 45px;
      line-height: 45px;
      text-align: center;  
      color: $white;
      font-family: "arenafont";
      display: block;      
      @include transition(all 0.3s ease-in-out 0s);
    }
    &:hover {
      text-decoration: none;
    }
  }
  &:hover{
    a {
      @include transparent($main-color, 1);
      &:before {        
        color: $white;
      }
    }
  }
}

/*menu on PC---------------------------------*/
.horizontal-menu {      
  background: none;  
  position: static;    
  .sidemenu-holder .navbar .navbar-collapse {
    .menu-list {      
      > li {
        &.dropdown {  
          > .dropdown-menu {                        
            top: 100%;
            left: 0;                                 
            @include dropdown-effect;
            @include translate(0, 10px);              
          }
          > .dropdown-inner > a,
          > a {
            &:after {
              content: '\e821';
              color: $main-color;
              height: 12px;
              line-height: 12px;
              font-family: arenafont;
              font-size: $font-small-icon;
              @include ab(n, 50%, -3px, n);
              @include translate(-50%, 10px);
              @include transition(all 0.35s ease 0s);
              @include opacity(0);
            }
          }
          &:hover,                    
          &.hover {
            > .dropdown-inner > a,
            > a { 
              position: relative;   
              &:after {
                @include opacity(1);
                @include translate(-50%, 0);
              }                      
            }
            > .dropdown-menu {              
              background: $nav-dropdown-background;                         
              @include dropdown-effect-hover;    
              z-index: 99999;                      
            }
          }
          &.mega-menu {
            position: static;                        
            > .dropdown-menu {                                   
              &.position-center {
                left: 50%;
                @include translate(-50%, 10px);
              }  
              &.position-right {
                left: auto;
                right: 0;
              } 
              .dropdown-menu {
                font-size: $nav-sub-font-size;                 
              } 
              &.column-4 {
                width: 100%;                
              }
              &.column-3 {
                width: 75%;                             
              }
              &.column-2 {
                width: 50%;               
              }
              &.column-1 {
                width: 25%;                
              }
            }
            &:hover,                    
            &.hover {
              > a { 
                position: relative;  
              }
              > .dropdown-menu {                    
                &.position-center {                  
                  @include translate(-50%, 0);
                }     
              }
            }
          }                  
        }       
        &:hover {          
          @include border-radius(0, 0, 0, 0);
          > .dropdown-inner > a,
          > a {
            color: $nav-link-hover-color;             
          }
