      }
    }
  }
  .tp-parallax-wrap {
    width: 100%;
    position: absolute !important;
    z-index: 1;
    &.image-1 {
      text-align: right;
      left: auto !important;
      right: -5%;
      top: -5% !important;
      max-width: 50%;
      @media (max-width: 1200px) {
        right: -2% !important;
        top: -2% !important;
      }
