@mixin swatch-product {    
  .swatch {
    margin: 15px 0 0;
    position: relative;      
    .header {
      padding: 0 10px 0 0;
      display: block;     
      margin: 0;
      line-height: 34px;                                            
      color: $body-title-color;     
      float: left;
      width: 110px;           
    }
    .swatch-element {
      margin: 0; padding: 0;        
      float: left;
      label {
        position: relative;
        background: transparent;                       
        margin: 0 5px 5px 0; 
        text-align: center;
        cursor: pointer;     
        min-width: 35px !important;
        height: auto !important;
        min-height: 35px;
        padding: 7px 5px;
        line-height: 19px;     
        color: $body-title-color;       
        font-size: $body-font-size - 1px;
        word-break: break-word;
        white-space: normal;
        font-weight: 700;
        text-transform: none;      
        background-repeat: no-repeat !important;
        background-position: center !important;
        background-size: cover;
        border: 1px solid $body-border-color;
        position: relative;
        @include border-radius-50;
        @include box-shadow(0 0 0 0 #ffffff inset);
        @include transition(all 0.35s ease 0s);          
      }
      input:checked + label {
        box-shadow: none;          
        cursor: default;   
        border-color: $main-color;
        color: $main-color;         
        @include box-shadow(0 0 0 0 #ffffff inset);
      }       
      &:hover { 
        @media (min-width: 1025px) {
          label{           
            border-color: $main-color;
            color: $main-color;
            @include box-shadow(0 0 0 0 #ffffff inset); 
          }
        }
      }
      &.soldout {
        label {
          cursor: default;
          background-color: transparent !important;
          color: $body-title-color !important; 
          border-color: $body-border-color !important;
        } 
        &:hover {
          @media (min-width: 1025px) {
            label {
              cursor: default;
              background-color: transparent !important;
              color: $main-color;  
            }
          }
        }
      }               
    }
    &.color {
      .swatch-element {
        label {
          &:after {
            content: '';
            width: 100%;
            height: 100%;
            background: $white;
            margin: 0 auto;
            @include ab(50%, 50%, n, n);
            @include translate(-50%, -50%);
            @include border-radius-circle;
            @include opacity(0);
            @include transition(all 0.35s ease 0s);
          }
        }
        input:checked + label {
          &:after {
            width: 10px;
            height: 10px;
            @include opacity(1);
          }
        }
        &.white {
          label {
            &:after {
              background: $black;
            }
          }
        }
        &:hover {
          @media (min-width: 1025px) {
            label {
              &:after {
                width: 10px;
                height: 10px;
                @include opacity(1);
              }
            }
          }
        }
      }
    }
    &.size {            
      .swatch-element {
        label {         
          @include border-radius-reset;
          background: $white;
          @include box-shadow(0 0 0 0 $white);            
        }     
        &:hover {
          @media (min-width: 1025px) {
            label {                    
              @include box-shadow(0 0 0 0 $white);
            }
          }            
        }
      }        
    }
    &:last-child {
      margin-bottom: 15px;
    }
  }
}
@mixin drawer-sidebar {
  position: fixed;
  z-index: 999999;
  left: 0;
  top: 0;
  height: 100%;
  width: 300px;
  padding: 30px 20px;
  overflow-y: auto;
  background: $body-background-color;
  @include translate(-300px, 0);  
  @include opacity(0);
  @include transition(all 0.45s cubic-bezier(0.25, 0.46, 0.45, 0.94));   
  @media (max-width: 300px) {
    width: 100%;
   padding-bottom: 20px;
  } 
  .f-close,
  .fof-close {    
    text-align: center;
    cursor: pointer;
    background: $body-second-color;
    @include border-radius-circle;
    @include transition(all 0.35s ease 0s);
    @include width-btn;
    @include ab(20px, n, n, 20px);    
    &:hover {
      background: $main-color;
      color: $main-color-text;
    }
  }
}
@mixin toggle-sidebar {    
  position: relative;
  width: 100%;
  display: none;
  padding: 25px 15px 0;
  margin: 0 auto 20px;
  background: $body-background-color;
  @include box-shadow(0 0 10px rgba($black, 0.05));
  .sb-title,
  .f-close {
    display: none;
  }      
  .sb-filter-wrapper {       
    width: 100%;     
  }     
  .sbf-title {
    > span {
      display: block;
    }
    > a {
      float: none !important;
      display: inline-block;
      margin-top: 5px;
    }
  }
  .sbw-filter > div {
    letter-spacing: -0.35em;
    vertical-align: top;
    .sb-filter {        
      padding: 0 15px;
      clear: none;
      float: none;
      display: inline-block;
      letter-spacing: 0;
      vertical-align: top;
      border: none;
      margin: 0 0 30px;        
    }
  }
}
@mixin icons-number {
  @include ab(n, n, -10px, -10px);
  @include border-radius-circle;
  width: 22px;
  height: 22px;
  line-height: 22px;
  background: $bg-number-icon;
  color: $text-number-icon;
  text-align: center;
  font-size: 12px;
  font-weight: 700;
}
@mixin hover-icons-number {
  @media (min-width: 1025px) { 
    -webkit-animation-name: hvr-icon-bob-float, hvr-icon-bob;
    animation-name: hvr-icon-bob-float, hvr-icon-bob;
    -webkit-animation-duration: .3s, .75s;
    animation-duration: .3s, 0.75s;
    -webkit-animation-delay: 0s, .3s;
    animation-delay: 0s, .3s;
    -webkit-animation-timing-function: ease-out, ease-in-out;
    animation-timing-function: ease-out, ease-in-out;
    -webkit-animation-iteration-count: 1, infinite;
    animation-iteration-count: 1, infinite;
    -webkit-animation-fill-mode: forwards;
    animation-fill-mode: forwards;
    -webkit-animation-direction: normal, alternate;
    animation-direction: normal, alternate;   
  }
}
@mixin padding-carousel {   
  .owl-stage-outer {
    padding: 5px 0 40px;
    margin-bottom: -40px;     
    @media (max-width: 1024px) {
      padding: 0;
      margin-bottom: 0;     
    }
  }
  .owl-dots {
    @media (min-width: 1025px) {
      bottom: 28px;
    }
  }
  .owl-controls .owl-nav {
    top: -40px;
  }
}
@mixin title-wrapper {
  padding-bottom: 13px;    
  line-height: 32px; 
  border-bottom: 1px solid $body-border-color; 
  position: relative;
  &:before {
    content: '';
    display: block;
    width: 115px;
    height: 2px;
    background: $main-color;
    @include ab(n, 0, -1px, 0);
  }
}
@mixin dropdown-effect {
  visibility: hidden;
  z-index: -99;
  display: block;
  @include translate(10px, 0);  
  @include opacity(0);
  @include transition(all 0.35s ease 0s);
}
@mixin dropdown-effect-hover {
  @include opacity(1);
  @include translate(0, 0);
  visibility: visible;
  z-index: 9999;
}
@mixin clearfix() {
  &:before,
  &:after {
    display: block;
    width: 100%;
    content: "";
    clear: both;
  }
}
