        position: static;
        padding: 0;
        background: none;
        text-align: left;
        @include opacity(1);
        @include translate(0, 0);   
      }
    }
  } 
  &:hover {
    @media (min-width: 1025px) {
      z-index: 5; 
      background: $white;
      &:after {
        @include rotateX(0);    
        @include opacity(1);
      }
      &:before {
        content: none;
      }
      .product-content {        
        .product-button {
          @include opacity(1);    
          @include translate(0, 0);                 
          z-index: 20;         
        }
        .pc-inner .price-cart-wrapper {          
          border-color: $body-border-color;           
        }
      }
    }         
  }  
  &.half-product-button {
    .product-content {      
      .product-button {
        > div {
          max-width: 100%;
        }
      }  
    } 
  }
  &.none-product-button {
    &:after {
      bottom: 0 !important;
    }
    .product-content {
      .price-cart-wrapper {
        border-color: transparent !important;
        padding-bottom: 0 !important;
      }
      .product-button {
        display: none;
      }  
    } 
  }
  &.product-owl-item {
    display: table;
    table-layout: fixed;
    vertical-align: top;       
    .product-head {
      float: none;
      display: table-cell;
      vertical-align: top;
