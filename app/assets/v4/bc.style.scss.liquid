.custom-block-video {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  padding-bottom: 56.25%;
  height: 0;
  height: auto;
  iframe {
    @include ab(0, 0, n, n);
    width: 100% !important;
    height: 100% !important;
    max-width: 100% !important;
    max-height: 100% !important;
    margin: 0 !important;
    -webkit-transition-duration: 0.35s !important;  
    transition-duration: 0.35s !important;
  }
  .mbYTP_wrapper {
    -webkit-transition-duration: 0.35s !important;  
    transition-duration: 0.35s !important;
    z-index: 2 !important;
  }
  .player {
    @include ab(0, 0, n, n);
    display: block !important;
    width: 100%;
    height: 100%;
    background-image: url({{'loading.gif' | asset_url }});  
    background-position: center center;
    background-repeat: no-repeat;
    z-index: 1;
    + button {
      @include opacity(0);
    }
    &.isMuted {
      background-image: none;
      + button {
        @include opacity(1);
      } 
    }
  }
  .buttonBar {
    display: none;
  }
  button {
    @include ab(50%, 50%, n, n);
    @include translate(-50%, -50%);    
    font-size: 0;    
    background: none;
    border: none;
    width: 80px;
    height: 80px;
    line-height: 50px;
    color: #222;
    text-align: center;
    z-index: 3;
    @include border-radius(3px, 3px, 3px, 3px);
    @include transparent($white, 0.5);
    @include transition(all ease-in-out 0.35s 0s);
    span {
      display: none;
    }
    &:before {
      font-size: 40px;   
      display: block;
    }
    &.b-play {
      &:before {
        content: '\ee906';
      }
    } 
    &.b-pause {
      @media (min-width: 1025px) {
        @include opacity(0);
      }      
      &:before {
        content: "\60";
        font-family: 'ElegantIcons';
      }
    }
  }
  &:hover {
    button.b-pause {
      @include opacity(1);
    }
  }
}
.image-layer {
  @include ab(50%, 0, n, 0);
  z-index: 2;
  max-height: 100%;
  max-width: 100%;
  margin: 0 auto;
  padding: 0 15px;
  @include translate(0, -50%);   
  @media (min-width: 768px) {
    width: 760px;
  }
  @media (min-width: 992px) {
    width: 980px;
  }
  @media (min-width: 1200px) {
    width: 1200px;
  }
  img {
    max-width: 100%;
    max-height: 100%;
    display: inline-block;  
    @media (min-width: 320px) and (max-width: 479px) {
      max-width: 69%;
    }
    @media (min-width: 480px) and (max-width: 767px) {
      max-width: 49%;
    }
    @media (min-width: 768px) and (max-width: 1024px) {
      max-width: 60%;
    }
    @media (min-width: 1025px) and (max-width: 1199px) {
      max-width:64%;
    }
  }
  &.layer-center {    
    text-align: center;    
  }
  &.layer-left {
    text-align: left;
  }
  &.layer-right {
    text-align: right;
  }
}
.parallax-section {
  background-repeat: no-repeat; 
  background-size: cover;
  background-position: 50% 50%;
  background-attachment: fixed;
  width: 100%;  
  position: relative;
  padding: 20% 0;
  margin: 0 auto;
  &.full-height {
    height: 100vh;
    @media (max-width: 1200px) {
      min-height: 80vh;
    }
    @media (max-width: 479px) {
      height: 60vh;
    }
    .heading-text {

    }
    .icon-layer {
      margin: 10px auto 13px;
    }
    .custom-block-heading-text {
      margin-bottom: 0;           

    }
    .custom-block-subheading-text {
      margin-bottom: 0;
      letter-spacing: 0.5px;
    }
    .button {
      margin-top: 2%;
      @media (min-width: 1025px) {
        &:hover {
          @include opacity(0.75);
        }
      }
    }
  }
  @media (max-width: 767px) {
    background-attachment: inherit;
    background-position: center center !important;    
  }
  .vcenter-container,
  .custom-item-container,
  .custom-item-inner-bgimage {
    display: table;
    height: 100%;
    table-layout: fixed;
    width: 100%;
    vertical-align: middle;
  }
  .vcenter,
  .text {
    display: table-cell;
    vertical-align: middle;
  }  
}
