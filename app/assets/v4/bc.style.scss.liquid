  &.navbar-responsive-menu {
    font-size: 0;
  }
}
#searchModal {  
  @include flexbox();
  background: rgba($black, 0.7) none repeat scroll 0 0;
  position: fixed;    
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
  align-items: center;
  -webkit-align-items: center; /* Safari 7.0+ */
  justify-content: center;
  display: none;
  padding: 10% 0 0 0;
  z-index: 9999;  
  #searchModal[aria-hidden="false"] {
    z-index: 99999;
  } 
  .modal-dialog {
    width: 80%;
    max-width: 520px;
    min-width: 290px;
    padding: 0;
    margin: 0 auto;
    text-align: center;
    position: relative;
    z-index: 3;
    background: $white; 
    @include border-radius-20;
    .modal-content {    
      @include border-radius-20;
      @include box-shadow(0 0 0 rgba(0, 0, 0, 0.5));
      border: none;
      padding: 30px;
      @media (max-width: 479px) {
        padding: 15px;
      }
      &.s-loading {
        .navbar-form {
          &:before {
            @media (min-width: 768px) {
              @include ab(12px, n, n, 0);
              width: 25px;
              height: 25px;            
              z-index: 5;          
              font-size: $font-small-icon; 
              vertical-align: middle; 
              line-height: 25px; 
              display: block;
              font-family: arenafont;
              content: "\e984";
              -webkit-animation: fa-spin 2s infinite linear;
              animation: fa-spin 2s infinite linear;
              color: $body-title-color; 
            }
          }
          .search-icon {
            @media (min-width: 768px) {
              @include opacity(0);
            }
          }
        }
      }
    }    
    button.close {        
      @include border-radius-circle;  
      @include opacity(1);
      @include box-shadow(0 0 0 rgba($black, 0.5));
      @include ab(-17px, n, n, -15px);                
      @include width-btn-1;      
      cursor: pointer;
      display: block;
      cursor: pointer;      
      text-align: center;
      z-index: 3;          
      font-size: 0;         
      &:before {
        @include close-btn;  
        color: $white;      
      }
      span {
        display: none;
      } 
      &:hover {
        background: $button-hover-btn1;
        &:before {
          color: $button-hover-btn1-txt;
        }
      }       
    }
    h3 {        
      margin: 0;
      padding: 0 0 15px;
      font-size: 18px;               
    }
    .navbar-form {
      padding: 0;
      margin: 0;
      position: relative;
      .form-control {
        width: 100%;        
        margin: 15px 0;   
        height: 42px; 
        padding: 10px 40px 10px 15px;
        @include border-radius-25;            
        &:focus {
          @include box-shadow(0 0 0 $white);
          border-color: $main-color;
        }
      }
      .search-icon {
        border: none;
        padding: 0;
        margin: 0;
        background: none;    
        width: 50px;
        height: 42px;
        text-align: center;
        line-height: 42px;
        @include ab(0, n, n, 0);   
        i {
          font-size: $font-small-icon - 4px;
          color: $body-title-color;
        }     
        &:hover {
          i {
            color: $main-color;
          }
        }
      }
    }
  }     
  #result-ajax-search {
    text-align: left;    
    width: 100%;          
    padding: 0; 
    margin: 15px 0 0;       
    display: none;  
    border: 2px solid $body-border-color;  
    overflow: hidden;
    @include box-shadow(0 0 0 rgba(0, 0, 0, 0.3));  
    @media (max-width: 767px) {
      display: none !important;
    }
    ul {
      margin: 0;
      padding: 0;
      max-height: 450px;
      overflow: auto ; 
      background: $white;   
      width: 100%;
      li {
        display: block;
        overflow: hidden;
        padding: 10px 15px;
        .search-item-img {
          float: left;
          margin: 0 15px 0 0; 
        }
        .search-item-title {
          display: block;           
          line-height: 1.4;            
          color: $body-title-product;            
          margin: 0 0 5px 115px;
          font-size: $product-font-size;
          .marked {
            color: $main-color;
          } 
          &:hover {
            color: $main-color;
          }           
        }            
        .price-compare {       
          margin: 0 10px 0 0;           
        } 
        p {
          margin: 0;                            
        }
        &:hover {
          background: #f6f6f6;
        }
      }
    }
  }
}
/* Sidebar cart ------------------------*/
.sb-progress {
  height: 18px;
  background: #707070;
  margin: 0 0 10px;
  @include border-radius-25;
  .sc-progress-bar,
  .sb-progress-bar {
    background: #222;
    display: block;
    color: $white;
    padding: 0 10px;
    text-align: center;
    line-height: 18px;
    background-image: -webkit-linear-gradient(bottom left, rgba(255,255,255,0.15) 25%, transparent 25%, transparent 50%, rgba(255,255,255,0.15) 50%, rgba(255,255,255,0.15) 75%, transparent 75%, transparent);
    background-image: -o-linear-gradient(bottom left, rgba(255,255,255,0.15) 25%, transparent 25%, transparent 50%, rgba(255,255,255,0.15) 50%, rgba(255,255,255,0.15) 75%, transparent 75%, transparent);
    background-image: linear-gradient(to top right, rgba(255,255,255,0.15) 25%, transparent 25%, transparent 50%, rgba(255,255,255,0.15) 50%, rgba(255,255,255,0.15) 75%, transparent 75%, transparent);
    background-size: 40px 40px;
    animation: move 2s linear infinite;
    @include border-radius-25;

  }
}
.progress-shipping-false {
  display: none;
}
.sb-text {
  padding: 0 0 20px;
