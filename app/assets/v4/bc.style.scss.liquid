            color: $main-color; 
            &:before {
              border-color: $main-color !important;
            }           
          }
        }
        &.active-filter {
          > a {
            color: $main-color !important;
            &:before {
              border-color: $main-color;         
            }
            &:after {
              content: "\e8ca";
              font-family: arenafont;
              font-size: 12px;
              width: 16px;
              height: 16px;               
              @include ab(2px, 2px, n, n);              
              @include opacity(1);
              @include transition(all 0.35s ease 0s);         
            }       
          }
        } 
        &.advanced-filter {
          line-height: 20px;
          margin-bottom: 13px;
          a {
            position: relative;
            padding-left: 25px;
            min-height: 20px;
            display: inline-block;    
            font-size: $body-font-size; 
            font-weight: 400;  
            color: $body-general-color;               
            &:before {
              content: "";
              @include ab(3px,0,n,n);              
              width: 16px;
              height: 16px;              
              border: 1px solid $body-border-color;  
              @include border-radius(3px, 3px, 3px, 3px);
              @include transition(all 0.35s ease 0s);            
            }
            &:after {
              content: "\e8ca";
              font-family: arenafont;
              font-size: 12px;
              width: 16px;
              height: 16px; 
              @include ab(2px, 2px, n, n);              
              @include opacity(0);
              @include transition(all 0.35s ease 0s);              
            }
            &:hover {
              @media (min-width: 1025px) {                
                color: $main-color;               
                &:before {
                  border-color: $main-color;
                }                
              }              
            }
          }
          &.af-color {
            border: none;
            padding: 0;
            float: none;            
            margin: 0 10px 10px 0;
            width: 17px;
            height: 17px;
            line-height: 17px;            
            position: relative; 
            cursor: pointer;
            float: left;     
            display: block;       
            border: 1px solid $body-border-color;            
            @include border-radius-circle;
            @include box-shadow(0 0 0 0 $body-border-color inset);  
            @include transition(all 0.35s ease 0s);
            a {                            
              padding: 0;
              position: relative;
              display: block;  
              font-size: 0;
              content: '';
              width: 100%;
              height: 100%; 
              min-height: 15px;
              &:before {
                content: none !important;
              }
              &:after {                              
                @include ab(50%, 50%, n, n);
                @include translate(-50%, -50%);
                @include border-radius-circle;
                width: 100%;
                height: 100%;
                background: $white;
                content: '';
              }
            }
            &:hover {
              @media (min-width: 1025px) {                
                @include box-shadow(0 0 0 0 $body-border-color inset); 
                a {
                  &:after {
                    width: 7px;
                    height: 7px;
                    @include opacity(1);
                  }
                }                
              }
            }
            &.active-filter {              
              @include box-shadow(0 0 0 0 $body-border-color inset); 
            }
            &.color_white {
              a {
                &:after {
                  background: $black;
                }
              }              
            }
          }
          &.active-filter {            
            a {
              &:before {
                border-color: $main-color;
              }
              &:after {
                width: 7px;
                height: 7px;
                @include opacity(1);
              }
            }
          }
        }
      }
    }    
  }  
}
